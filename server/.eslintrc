{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
    	"eslint:recommended",
    	"prettier"
    ],
    "parserOptions": {
       "ecmaVersion": 2020
   	},
    "plugins": [
	  "prettier"
	],
    "rules": {
    	"prettier/prettier": "error",
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ]
      
    }
}